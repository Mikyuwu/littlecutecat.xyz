---
import ViewTransitions from 'astro/components/ViewTransitions.astro';

interface Props {
    title: string;
}

const { title } = Astro.props;
---

<!doctype html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{title}</title>
        <ViewTransitions>
    </head>
    <body><slot /></body>
</html>